{
  "name": "observe-2ch-server",
  "version": "0.1.0",
  "description": "patrol twitter observe board at 2ch",
  "main": "app/server/index.js",
  "engines": {
    "node": "^8.*",
    "npm": "^5.*"
  },
  "license": "MIT",
  "author": "gutchom",
  "repository": {
    "type": "git",
    "url": "https://github.com/gutchom/observe-2ch-server"
  },
  "bugs": {
    "url": "https://github.com/gutchom/observe-2ch-server/issues"
  },
  "scripts": {
    "start": "npm-run-all -p watch docker",
    "test": "mocha --require ts-node/register ./test/**/*.test.ts",
    "clean": "rm -rf ./app/*",
    "migrate": "db-migrate up -e development",
    "docker": "docker-compose up",
    "prewatch": "npm run clean",
    "watch": "tsc && tsc -w --pretty --sourceMap",
    "prebuild": "npm run clean",
    "build": "tsc && cd ./client && npm run build",
    "postbuild": "cd ../",
    "predev": "npm run migrate",
    "dev": "pm2-dev process.yml --watch"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "connect-redis": "^3.3.2",
    "cookie-parser": "^1.4.3",
    "db-migrate": "^0.10.0-beta.24",
    "db-migrate-mysql": "^1.1.10",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "moment": "^2.19.0",
    "mysql2": "^1.4.2",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "pm2": "^2.7.1",
    "pug": "^2.0.0-rc.4",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^4.13.8",
    "sequelize-typescript": "^0.5.0",
    "text-encoding": "^0.6.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.15",
    "@types/body-parser": "^1.16.5",
    "@types/cheerio": "^0.22.3",
    "@types/colors": "^1.1.3",
    "@types/connect-redis": "0.0.6",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.0.37",
    "@types/express-session": "^1.15.3",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.33",
    "@types/passport": "^0.3.4",
    "@types/passport-twitter": "^1.0.32",
    "@types/power-assert": "^1.4.29",
    "@types/redis": "^2.6.0",
    "@types/sinon": "^2.3.5",
    "@types/text-encoding": "0.0.32",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "power-assert": "^1.4.4",
    "sinon": "^4.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3"
  }
}
